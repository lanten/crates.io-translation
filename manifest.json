{
  "name": "crates.io Translation",
  "description": "Allow browser translation of the main area of ​​crates.io \n\n 允许浏览器对 crates.io 的主要区域进行翻译",
  "version": "1.0",
  "manifest_version": 3,
  "host_permissions": [
    "https://crates.io/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://crates.io/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "icons/icon.png"
  }
}